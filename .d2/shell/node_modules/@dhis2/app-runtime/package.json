{
    "name": "@dhis2/app-runtime",
    "description": "A singular runtime dependency for applications on the DHIS2 platform",
    "version": "2.8.0",
    "main": "./build/cjs/index.js",
    "module": "./build/es/index.js",
    "types": "./build/types/index.d.ts",
    "exports": {
        "import": "./build/es/index.js",
        "require": "./build/cjs/index.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dhis2/app-runtime.git",
        "directory": "runtime"
    },
    "author": "Austin McGee <austin@dhis2.org>",
    "license": "BSD-3-Clause",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "build/**"
    ],
    "dependencies": {
        "@dhis2/app-service-config": "2.8.0",
        "@dhis2/app-service-data": "2.8.0",
        "@dhis2/app-service-alerts": "2.8.0"
    },
    "peerDependencies": {
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6"
    },
    "scripts": {
        "build:types": "tsc --emitDeclarationOnly --outDir ./build/types",
        "build:package": "d2-app-scripts build",
        "build": "concurrently -n build,types \"yarn build:package\" \"yarn build:types\"",
        "test": "echo \"No tests yet!\""
    }
}
